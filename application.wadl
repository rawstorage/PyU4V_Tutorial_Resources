      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[the 12 digit local Symmetrix ID which will be used to query for the SRDF storage groups]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="storageGroupId" style="template">
        <wadl:doc>
          <![CDATA[the ID of the SRDF Storage Group]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="rdfgNumId" style="template">
        <wadl:doc>
          <![CDATA[the SRDF Group Number]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call looks up an SRDF Protected Storage Groups on a Symmetrix]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[object containing SRDF Storage group properties]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="replication84:storageGroupRdfg" xmlns:replication84="http://www.emc.com/em/2012/07/univmax/restapi/84/replication"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="PUT">
        <wadl:doc>
          <![CDATA[This call executes Storage Group level SRDF actions.  There are specific options for the action in the body object]]>
        </wadl:doc>
        <wadl:request>
          <wadl:representation mediaType="application/xml" element="replication84:storageGroupRdfUpdate" xmlns:replication84="http://www.emc.com/em/2012/07/univmax/restapi/84/replication"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:representation mediaType="application/xml" element="replication84:storageGroupRdfg" xmlns:replication84="http://www.emc.com/em/2012/07/univmax/restapi/84/replication"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="DELETE">
        <wadl:doc>
          <![CDATA[delete SRDF pairing for a particular RDFG]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="half_delete" style="query">
            <wadl:doc>
              <![CDATA[is the command a Half Delete (as opposed to full)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="hop2" style="query">
            <wadl:doc>
              <![CDATA[is this to run on the Hop2 leg]]>
            </wadl:doc>

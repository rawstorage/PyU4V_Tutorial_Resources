        <wadl:doc>
          <![CDATA[Retrieves the specified CU image.]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[a representation of the requested CU image.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning84:cuImage" xmlns:sloprovisioning84="http://www.emc.com/em/2012/07/univmax/restapi/84/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="PUT">
        <wadl:doc>
          <![CDATA[Modifies the specified CU Image. Supported modifications are the mapping/unmapping of volumes and
the assigning/removing of an alias address range on the CU image. NOTE: Only one modification type can be
performed at a time.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[contains information on the mapping or alias change to perform on the CU image,
either the mapping of additional volumes or the removal of existing mappings or
the assignment or removal of an alias address range.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning84:editCUImageParam" xmlns:sloprovisioning84="http://www.emc.com/em/2012/07/univmax/restapi/84/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[a [@code Job} instance is returned if the ASYNCHRONOUS execution option was selected otherwise
by default the modified CuImage resource will be returned.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning84:cuImage" xmlns:sloprovisioning84="http://www.emc.com/em/2012/07/univmax/restapi/84/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

    <wadl:resource path="/restapi/91/sloprovisioning/symmetrix/{symmetrixId}/host">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[This call creates a new Host on a specified Symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[The CreateHostParam parameter]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:createHostParam" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[a [@code Job} instance is returned if the ASYNCHRONOUS execution option was selected otherwise
by default the newly created Host resource will be returned.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:host" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of host names for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="host_group_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host names that are associated with the specified host Groups only]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_masking_views" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host names that contain greater than(">1"), Less than("<1") or equal to the specified number of masking views]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_host_groups" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host names that contain greater than(">1"), Less than("<1") or equal to the specified number of host Groups]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_initiators" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host names that contain greater than(">1"), Less than("<1") or equal to the specified number of initiators]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="powerPathHostId" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host names that are associated with the specified powerPathHostId
e.g. equal to "powerPathHostId=abc" or contains "powerPathHostId=<like>a"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_powerpath_hosts" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host names that contain greater than("num_of_powerpath_hosts=>1"), Less than("num_of_powerpath_hosts=<1") or
equal to the specified num_of_powerpath_hosts]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="initiator_name" style="query"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Host names]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:listHostResult" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/91/sloprovisioning/symmetrix/{symmetrixId}/hostgroup">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[This call creates a new Host Group on a specified Symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[The CreateHostGroupParam parameter]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:createHostGroupParam" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[a [@code Job} instance is returned if the ASYNCHRONOUS execution option was selected otherwise
by default the newly created HostGroup resource will be returned.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:hostGroup" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of host groups for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="host_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host group names that contain the specified hosts only]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_masking_views" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host group names that contain greater than(">1"), Less than("<1") or equal to the specified number of masking views]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_hosts" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host group names that contain greater than(">1"), Less than("<1") or equal to the specified number of hosts]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Host Group names]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:listHostGroupResult" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/91/sloprovisioning/symmetrix/{symmetrixId}/initiator">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of Initiator ids (director:port:hba) for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="directorId" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that contain the specified Directors only. Format:"FA-7E"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="portId" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that contains the specified ports only. Format:"0"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="initiator_hba" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that contain the specified initiator HBA only.]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_masking_views" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that contain greater than(">1"), Less than("<1") or equal to the specified number of masking views]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_host_groups" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that contain greater than(">1"), Less than("<1") or equal to the specified number of Host groups]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_vols" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that contain greater than(">1"), Less than("<1") or equal to the specified number of volumes]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="alias" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that have a specified alias name
e.g. equal to "alias=alias1" or contains "alias=< like>alias"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="port_flag_overrides" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that have port Flags overriden only]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="on_fabric" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that are on the Fabric only  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="logged_in" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that are logged in only (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="iscsi" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that are iscsi only (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="in_a_host" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Initiator Ids that are in_a_host only (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="powerPathHostId" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host names that are associated with the specified powerPathHostId
e.g. equal to "powerPathHostId=abc" or contains "powerPathHostId=<like>a"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_powerpath_hosts" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Host names that contain greater than("num_of_powerpath_hosts=>1"), Less than("num_of_powerpath_hosts=<1") or
equal to the specified num_of_powerpath_hosts]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="fabric_name" style="query"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Initiator ids (director:port:hba)]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:listInitiatorResult" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/91/sloprovisioning/symmetrix/{symmetrixId}/maskingview">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of masking view names for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="host_or_host_group_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Masking View names that contain the specified hosts or Host Groups only]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="port_group_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Masking View names that contain the specified Port Groups only]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="storage_group_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Masking View names that contain the specified Storage Groups only]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="protocol_endpoint_masking_view" style="query">

            </wadl:doc>
          </wadl:param>
          <wadl:param name="symforce" style="query">
            <wadl:doc>
              <![CDATA[is the symforce option to be used, use this with care]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="star" style="query">
            <wadl:doc>
              <![CDATA[are there volumes in a Star configuration]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="bypass" style="query"/>
          <wadl:param name="keepR1" style="query"/>
          <wadl:param name="keepR2" style="query"/>
        </wadl:request>
      </wadl:method>
    </wadl:resource>

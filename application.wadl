          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:representation mediaType="application/xml" element="replication91:rdfGroup" xmlns:replication91="http://www.emc.com/em/2012/07/univmax/restapi/91/replication"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

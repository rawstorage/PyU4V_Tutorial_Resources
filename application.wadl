    <wadl:resource path="/restapi/91/vvol/symmetrix/{symmetrixId}/storagecontainer/{storageContainerId}/storageresource">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="storageContainerId" style="template">
        <wadl:doc>
          <![CDATA[The Storage Container ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of Storage Resource Ids for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="subscribed_limit_gb" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Resource Ids that contain greater than(">1"), Less than("<1") or equal to the specified number of Storage Container Limit (GB)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="subscribed_free_gb" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Resource Ids that contain greater than(">1"), Less than("<1") or equal to the specified number of Storage Container Free (GB)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="subscribed_used_gb" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Resource Ids that contain greater than(">1"), Less than("<1") or equal to the specified number of Storage Container Used (GB)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="srp" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Resource Ids  that have a description associated
e.g. equal to "srp=srp1" or contains "srp=< like>srp"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="slo" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Resource Ids  that have a description associated
e.g. equal to "slo=slo1" or contains "slo=< like>slo"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="workload" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Resource Ids  that have a description associated
e.g. equal to "workload=workload1" or contains "workload=< like>workload"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="uuid" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Resource Ids  that have a description associated
e.g. equal to "uuid=6000097000000bbf821200049a0d0002" or contains "uuid=< like>60000"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="compression" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Resources that are compression enabled/disabled]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Storage Resource names]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="vvol91:listStorageResourceResult" xmlns:vvol91="http://www.emc.com/em/2012/07/univmax/restapi/91/vvol"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/91/wlp/symmetrix/{symmetrixId}/storagegroup/{storageGroupId}/changeslo">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[- Target Symmetrix ID for workloads]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="storageGroupId" style="template"/>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Test performance impact of modifying an existing storage group.
Generate admissibility scores and recommended front-end ports for a given workload or workloads. Admissibility scores
are a representation of the observed performance impact on the system before and the predicted impact after a new
workload is added to the system. The scores are separated into Front End, Back End, Cache, and (if applicable) RDF scores.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[Contains the name of the storage group, and ChangeSLOParam.
ChangeSLOParam consists of the new SLO name, the new SRP name, scale factor]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="wlp91:changeSLOParam" xmlns:wlp91="http://www.emc.com/em/2012/07/univmax/restapi/91/wlp"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
          <wadl:param name="details" style="query">
            <wadl:doc>
              <![CDATA[Show individual scores for all 42 buckets. A bucket is a four hour time window.
A week has 42 buckets (0-indexed). Bucket 0 is Sunday 12am-4am, 1 is Sunday 4am-8am, etc.]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[An AdmissibilityResult]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="wlp91:admissibilityResult" xmlns:wlp91="http://www.emc.com/em/2012/07/univmax/restapi/91/wlp"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

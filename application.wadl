        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Masking View names]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:listMaskingViewResult" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[This call creates a new masking view on a specified Symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[The CreateMaskingViewParam parameter]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:createMaskingViewParam" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[a [@code Job} instance is returned if the ASYNCHRONOUS execution option was selected otherwise
by default the newly created MaskingView resource will be returned.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning91:maskingView" xmlns:sloprovisioning91="http://www.emc.com/em/2012/07/univmax/restapi/91/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>

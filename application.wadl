    <wadl:resource path="/restapi/90/wlp/symmetrix/{symmetrixId}/utilization">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[- The Symmetrix Array ID.]]>
        </wadl:doc>

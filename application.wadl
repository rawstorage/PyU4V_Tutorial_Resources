          <wadl:doc>
            <![CDATA[Component utilizations.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="wlp90:maxArrayComponentUtilizationList" xmlns:wlp90="http://www.emc.com/em/2012/07/univmax/restapi/90/wlp"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>

          <wadl:doc>
            <![CDATA[A list of SRPs names]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning84:listSRPResult" xmlns:sloprovisioning84="http://www.emc.com/em/2012/07/univmax/restapi/84/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/84/sloprovisioning/symmetrix/{symmetrixId}/storagegroup">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Retrieves the storage groups configured on the specified Symmetrix.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="storageGroupId" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that equal or contain the specified storage Group name only.
e.g. equal to "storageGroupId=gold_SG" or contains "storageGroupId=<like>gold"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="volumeId" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that contain the specified volumes names only.
e.g. equal to "volumeId=00123" or contains "volumeId=<like>12"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="slo_compliance" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names with one of the following slo_compliance : none, stable, marginal or  critical]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="cap_gb" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that contain greater than("cap_gb=>1"), Less than("cap_gb=<1") or
equal to the specified Capacity(GB)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_vols" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that contain greater than("num_of_vols=>1"), Less than("num_of_vols=<1") or equal to the specified number of volumes]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_child_sgs" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that contain greater than("num_of_child_sgs=>1"), Less than("num_of_child_sgs=<1") or equal to the specified number of Child Storage Group]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_parent_sgs" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that contain greater than("num_of_parent_sgs=>1"), Less than("num_of_parent_sgs=<1") or equal to the specified number of Parent Storage Groups]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_snapshots" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that contain greater than("num_of_snapshots=>1"), Less than("num_of_snapshots=<1") or equal to the specified number of Snap VX Snapshots]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_masking_views" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that contain greater than("num_of_masking_views=>1"), Less than("num_of_masking_views=<1") or equal to the specified number of Masking Views]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="parent" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that are parent Storage groups only  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="child" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that are child Storage groups only  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="srp_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that have a SRP  name associated
e.g. equal to "srp_name=srp1" or contains "srp_name=<like>srp"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="slo_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that have a SLO  name associated
e.g. equal to "slo_name=slo1" or contains "slo_name=<like>slo"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="child_sg_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names based on the specified child Storage Groups only,
More than one child Storage Group can be specified
e.g. "child_sg_name=childSG1&child_sg_name=childSG2" returns storage groups
where child_sg_name is childSG1 OR where child_sg_name is childSG2]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="parent_sg_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that have a specified Parent Storage Group Name
e.g. equal to "parent_sg_name=parent1" or contains "parent_sg_name=<like>parent"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="ckd" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display the names of storage groups
that contain CKD volumes, or in the case of a parent storage group when a
child group contains a CKD volume (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="fba" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display the names of storage groups
that contain FBA volumes, or in the case of a parent storage group when a
child group contains a FBA volume (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="compression" style="query">
            <wadl:doc>
              <![CDATA[Filtering of SGs with compression on/off (Optional)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="compression_ratio_to_one" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that contain greater than("compression_ratio_to_one=>1"), Less than("compression_ratio_to_one=<1") or equal to the specified compression ratio to one]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="base_slo_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group names that have a specified Base Slo Name
e.g. equal to "base_slo_name=bronze" or contains "base_slo_name=<like>bro"]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Storage Group names]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning84:listStorageGroupResult" xmlns:sloprovisioning84="http://www.emc.com/em/2012/07/univmax/restapi/84/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[This call creates a new Storage Group on a specified Symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[The CreateStorageGroupParam parameter]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning84:createStorageGroupParam" xmlns:sloprovisioning84="http://www.emc.com/em/2012/07/univmax/restapi/84/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[a [@code Job} instance is returned if the ASYNCHRONOUS execution option was selected otherwise
by default the newly created StorageGroup resource will be returned.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning84:storageGroup" xmlns:sloprovisioning84="http://www.emc.com/em/2012/07/univmax/restapi/84/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/84/sloprovisioning/symmetrix/{symmetrixId}/volume">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of volumes ids for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="storageGroupId" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids from a single Storage Group only.
e.g. "storageGroupId=storageGroupId"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="encapsulated_wwn" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain the specified volume encapsulated_wwn.
e.g. "encapsulated_wwn=encapsulated_wwn1&encapsulated_wwn=< like>encapsulated_wwn"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="wwn" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain the specified volume wwn.
e.g. "wwn=1234567891234568"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="symmlun" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified symmlun]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="status" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain the specified volume status.
e.g. "status=status1&status=< like>status2"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="physical_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain the specified volume physical_name.]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="volume_identifier" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain the specified volume volume_identifier.]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="allocated_percent" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified allocated_percent]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="cap_tb" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified cap_tb]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="cap_gb" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified cap_gb]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="cap_mb" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified cap_mb]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="cap_cyl" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified cap_cyl]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_storage_groups" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified num_of_storage_groups]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_masking_views" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified num_of_masking_views]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="num_of_front_end_paths" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that contain greater than(">1"), Less than("<1") or equal to the specified num_of_front_end_paths]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="virtual_volumes" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are virtual_volumes  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="private_volumes" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are private_volumes (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="available_thin_volumes" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are available_thin_volumes (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="tdev" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are tdev (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="thin_bcv" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are thin_bcv(true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="vdev" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are vdev (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="gatekeeper" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are gatekeeper  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="data_volume" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are data_volume  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="dld" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are dld  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="drv" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are drv (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="mapped" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are mapped (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="bound_tdev" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are bound_tdev  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="reserved" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are reserved  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="pinned" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are pinned  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="encapsulated" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are encapsulated  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="associated" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are associated  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="emulation" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are of the specified emulation.
e.g. "emulation=CKD-3390&emulation=<like>FBA"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="split_name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are mapped to CU images associated to the specified FICON split.
e.g. "split_name=split0&split_name=<like>split"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="cu_image_num" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are mapped to a CU image with the specified CU image number.
e.g. "cu_image_num=0x01&cu_image_num=<like>0x01"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="cu_image_ssid" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are mapped to a CU image with the specified CU SSID.
e.g. "cu_image_ssid=0x2100&cu_image_num=<like>0x2100"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="rdf_group_number" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
volume ids that are part of the specified rdf group.]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A session based Iterator which has 1000 volume Ids per iteration, expires after 10 minutes,
and can be queried from common resource using URL: /common/Iterator/{iteratorId}, alternatively an empty list iterator.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="common:iterator" xmlns:common="http://www.emc.com/em/2012/07/univmax/restapi/common"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/84/sloprovisioning/symmetrix/{symmetrixId}/workloadtype">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of Workloads for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Workload names]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="sloprovisioning84:listWorkloadTypeResult" xmlns:sloprovisioning84="http://www.emc.com/em/2012/07/univmax/restapi/84/sloprovisioning"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/84/system/symmetrix/{symmetrixId}/alert">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of Alert ids  for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="type" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Alert Ids that has the following severity: ARRAY, PERFORMANCE, SERVER]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="description" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Alerts that have the specified description e.g. equal to "description=description1" or contains "description=< like>description"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="severity" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Alert Ids that has the following severity: NORMAL, INFORMATION, MINOR, WARNING, CRITICAL, FATAL]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="state" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Alert Ids that has the following state: NEW, ACKNOWLEDGED, CLEARED]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="created_date" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Alert Ids that are greater than(">1"), Less than("<1") or equal to the specified created_date "MMM-dd-yyyy HH:mm:ss.SSS"]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="created_date_milliseconds" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Alert Ids that contain greater than(">1"), Less than("<1") or equal to the specified date in milliseconds]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="acknowledged" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Alert Ids that are acknowledged or not  (true/false)]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Alert Ids]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="system84:listAlertResult" xmlns:system84="http://www.emc.com/em/2012/07/univmax/restapi/84/system"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/84/system/symmetrix/{symmetrixId}/job">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call queries for a list of Job ids for the specified symmetrix]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="name" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Jobs that have a Job name associated
e.g. equal to "name=name1" or contains "name=< like>name"]]>
            </wadl:doc>
          </wadl:param>

      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[the Symmetrix array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="splitId" style="template">
        <wadl:doc>
          <![CDATA[the FICON split id]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="cuImageSSID" style="template">
